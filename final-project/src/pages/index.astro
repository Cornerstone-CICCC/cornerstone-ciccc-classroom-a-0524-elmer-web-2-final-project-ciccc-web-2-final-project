---
import BaseLayout from "../layouts/BaseLayout.astro"
import '../styles/global.css' 
import ServiceSection from "../components/ServiceSection.astro"
import RequestQuote from "../components/RequestQuote.astro"
import OurTeam from "../components/OurTeam.astro"
import ContactUs from "../components/ContactUs.astro"
import Subscribe from "../components/Subscribe.astro"
import HeroCard from "../components/HeroCard.astro"
import AboutCard from "../components/AboutCard.astro"

const title = "Home"
---
<BaseLayout pageTitle={title}>
    <main>
        <HeroCard/>
        <ServiceSection/>
        <AboutCard/>
        <OurTeam/>
        <ContactUs/>
        <RequestQuote/>
        <Subscribe/> 
    </main>
</BaseLayout>